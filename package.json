{
  "name": "spotify-adapter",
  "display_name": "Spotify",
  "version": "0.1.2",
  "description": "Control your spotify player",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/spotify-adapter.git"
  },
  "author": "Tim Hellhake",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/tim-hellhake/spotify-adapter/issues"
  },
  "homepage": "https://github.com/tim-hellhake/spotify-adapter#readme",
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "spotify-adapter.js"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "schema": {
      "type": "object",
      "required": [
        "accessToken"
      ],
      "properties": {
        "accessToken": {
          "type": "string",
          "title": "The spotify access token"
        }
      }
    }
  },
  "dependencies": {
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0"
  }
}
